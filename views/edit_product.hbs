                                <div id="product_attributes" class="panel woocommerce_options_panel"
                                    style="display: none;">
                                    <div class="toolbar toolbar-top">
                                        <span class="expand-close">
                                            <a href="#" class="expand_all">Expand</a> / <a href="#"
                                                class="close_all">Close</a>
                                        </span>
                                        <select name="attribute_taxonomy" class="attribute_taxonomy">
                                            {{#each attributes as |value key|}}
                                            {{#if value.terms}}
                                            <option value="{{value.slug}}" data-rel="{{key}}"
                                                {{#termcheck value.slug ../attributeselected}} disabled="disabled"
                                                {{/termcheck}}>{{value.name}}</option>
                                            {{/if}}
                                            {{/each}}
                                        </select>
                                        <button type="button"
                                            class="button add_attribute btn btn-primarys button">Add</button>
                                    </div>
                                    <div class="product_attributes wc-metaboxes ui-sortable">

                                        {{#each newattribute as |value key|}}
                                        {{#each ../products.attributes as |selectedattributes attrkey|}}
                                        {{#equal value.slug selectedattributes.names}}
                                        <div data-taxonomy="{{value.slug}}" class="woocommerce_attribute" rel="{{key}}"
                                            id="attribute-{{value.slug}}" data-terms-selected="">
                                            <h3>
                                                <a href="javascript:void(0)"
                                                    class="btn btn-danger remove_row delete">Remove</a>
                                                <div class="handlediv" title="Click to toggle"></div>
                                                <strong class="attribute_name">{{value.name}}</strong>
                                            </h3>
                                            <div class="woocommerce_attribute_data wc-metabox-content" style="">
                                                <table cellpadding="0" cellspacing="0">
                                                    <tbody>
                                                        <tr>
                                                            <td class="attribute_name">
                                                                <label>Name:</label><strong>{{value.name}}</strong><input
                                                                    type="hidden" name="attribute[{{key}}][names]"
                                                                    value="{{value.slug}}"> </td>
                                                            <td rowspan="3">
                                                                <label>Value(s):</label>
                                                                <select multiple="multiple"
                                                                    class="multiselect attribute_values js-example-basic-multiple"
                                                                    name="attribute[{{key}}][values]">
                                                                    {{#each value.terms as |termsvalue termskey|}}
                                                                    <option value="{{termsvalue.slug}}"
                                                                        {{#termcheck termsvalue.slug selectedattributes.values}}selected="selected"
                                                                        {{/termcheck}}>{{termsvalue.name}}</option>
                                                                    {{/each}}
                                                                </select>
                                                                <button
                                                                    class="btn btn-primary button plus select_all_attributes"
                                                                    type="button">Select all</button>
                                                                <button
                                                                    class="btn btn-secondary button minus select_no_attributes"
                                                                    type="button">Select none</button>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                        {{/equal}}
                                        {{/each}}
                                        {{/each}}
                                    </div>
                                    <div class="toolbar attribute_add_btn">
                                        <button type="button"
                                            class="btn btn-success button save_attributes button-primary"
                                            data-pagetype="editproduct">Save attributes</button>
                                    </div>
                                </div>
                                <div id="variable_product_options" class="panel woocommerce_options_panel"
                                    style="display: none;">
                                    <div id="variable_product_options_inner">
                                        {{#if products.variation}}
                                        <div class="variation-wrapper" data-variation-rel="1">
                                            <div class="toolbar toolbar-top variable-add-toolbar">
                                                <a class="btn btn-primary button bulk_edit do_variation_action"
                                                    data-variation-rel="{{variationcount}}">Add Variation</a>
                                                <div class="clear"></div>
                                            </div>
                                            <div class="woocommerce_variations" data-attributes="">
                                                {{#each products.variation as |variationvalue variationkey|}}
                                                <div class="woocommerce_variation wc-metabox open custom-use-select Small shadow"
                                                    id="product_variation-{{variationkey}}">
                                                    <h3>
                                                        <a href="javascript:void(0)"
                                                            class="btn btn-danger remove_variation delete"
                                                            rel="product_variation-{{variationkey}}">Remove</a>
                                                        <div class="handlediv" aria-label="Click to toggle"></div>
                                                        <div class="tips sort"></div>
                                                        <div class="variation_fields_section"></div>
                                                        {{#each ../products.attributes as |attrvalue attrkey|}}
                                                        <select
                                                            name="variaton_atttribute[{{variationkey}}][{{attrvalue.names}}]"
                                                            class="variation_attribute_name">
                                                            <option value="">Please Select {{attrvalue.names}}</option>
                                                            {{#each attrvalue.values as |termsvalue termskey|}}
                                                            <option value="{{termsvalue}}"
                                                                {{#variationmatch attrvalue.names termsvalue variationvalue}}selected="selected"
                                                                {{/variationmatch}}>{{termsvalue}}</option>
                                                            {{/each}}
                                                        </select>
                                                        {{/each}}
                                                        <span class="form_error attribute_error">Please Select Variation
                                                            Attribute Name</span>
                                                    </h3>
                                                    <div class="woocommerce_variable_attributes wc-metabox-content">
                                                        <div class="data">
                                                            <p
                                                                class="form-field variable_bar{{variationkey}}_field form-row">
                                                                <label for="variable_bar{{variationkey}}">
                                                                    <abbr title="Stock Keeping Unit">Barcode</abbr>
                                                                </label>
                                                                <input type="text" class="short variable_bar_field"
                                                                    name="variaton_atttribute[{{variationkey}}][bar]"
                                                                    id="variable_bar{{variationkey}}"
                                                                    value="{{variationvalue.bar}}" placeholder="">
                                                                <span class="form_error">Please Enter a Barcode</span>
                                                            </p>
                                                            <p
                                                                class="form-field variable_sku{{variationkey}}_field form-row">
                                                                <label for="variable_sku{{variationkey}}">
                                                                    <abbr title="Stock Keeping Unit">SKU</abbr>
                                                                </label>
