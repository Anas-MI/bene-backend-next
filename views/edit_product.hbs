{{> header}}
{{> nav}}
<div id="page-wrapper" style="padding-left: 15px;padding-right: 15px">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Edit Product</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12 col-md-12">
            <form method="POST" action="/products/edit/{{products.id}}" enctype="multipart/form-data"
                class="product-add-form" id="editproductform">
                <div class="col-md-8 shadow-lg custom-division">
                    <div class="page_notice_section">
                        <div class="alert alert-success productsuccess" style="display:none;"></div>
                        <div class="alert alert-danger productdanger" style="display:none;"></div>
                        <div class="form_error sku_error">Product SKU Can not be Left Blank</div>
                        <div class="form_error regular_price_error">Please enter the Default Regulare Price</div>
                        <div class="form_error attributeserror">Attributes Section Can not be Left Blank</div>
                        <div class="form_error variationserror">Variation Section Can not be Left Blank</div>
                    </div>
                    <div class="title-descripion">
                        <div id="form-group">
                            <label>Edit Serial number:</label>
                            <input class="form-control" name="productid" type="text" id="editproductid"
                                autocomplete="off" value="{{products.productid.id}}"
                                data-prodmeta-id="{{products._id}}">
                        </div>
                        <div id="form-group">
                            <label>Title: <span class="required">*</span></label>
                            <input class="form-control editproducttitle" name="producttitle" type="text"
                                autocomplete="off" value="{{products.productid.producttitle}}">
                            <span class="form_error">Please enter Product Name</span>
                        </div>
                        {{!-- <div id="form-group">
                            <label>Barcode: <span class="required">*</span></label>
                            <input class="form-control editbarcode" name="barcode" type="text" autocomplete="off"
                                value="{{products.barcode}}">
                            <span class="form_error">Please enter the barcode number</span>
                        </div> --}}
                        <div id="form-group">
                            <label>Short Description:</label>
                            <textarea class="form-control" name="sdescription" id="productbody" row="10"
                                column="10">{{products.productid.sdescription}}</textarea>
                        </div>
                        <div id="form-group">
                            <label>Long Description:</label>
                            <textarea class="form-control" name="description" id="productbody" row="10"
                                column="10">{{products.productid.description}}</textarea>
                        </div>
                    </div>
                    <div id="woocommerce-product-data" class="postbox ">
                        <h2 class="hndle ui-sortable-handle">
                            <span>Product data<span class="type_box ">
                                    <label for="product-type">
                                        <select id="product-type" name="producttype">
                                            <optgroup label="Product Type">
                                                {{#select products.producttype}}
                                                <option value="simple">Simple product</option>
                                                <option value="variable">Variable product</option>
                                                {{/select}}
                                            </optgroup>
                                        </select>
                                    </label>
                                </span>
                            </span>
                        </h2>
                        <h2 class="hndle ui-sortable-handle">
                            <span>Vendor ID: <span class="required"></span><span class="type_box ">
                                    <label for="visibility-type">
                                        <select name="vendorid" class="">
                                            {{#each vendor as |filter|}}
                                            <option value="{{filter._id}}"
                                                {{#equal ../products.vendorid._id filter._id}}selected="selected"
                                                {{/equal}}>{{filter.vendorid}}: {{this.vendorname}}</option>
                                            {{/each}}
                                        </select>

                                    </label>
                                </span>
                            </span>
                            <span class="form_error">Please enter the Product visibility</span>
                        </h2>

                        <h2 class="hndle ui-sortable-handle">
                            <span>Packaging Type:<span class="required"></span><span class="type_box ">
                                    <label for="visibility-type">

                                        <select name="packagetype" class="pagemenu">

                                            {{#each ptype as |filter|}}
                                            <option value="{{filter._id}}"
                                                {{#equal ../products.packagingtype._id filter._id}}selected="selected"
                                                {{/equal}}>{{filter.packingid}}: {{this.packingtype}}</option>
                                            {{/each}}
                                        </select>
                                    </label>
                                </span>
                            </span>
                            <span class="form_error">Please enter the Product visibility</span>
                        </h2>
                        <h2 class="hndle ui-sortable-handle">
                            <span>Visibility <span class="required">*</span><span class="type_box">
                                    <label for="visibility-type">
                                        <select id="visibility-type" class="edit_prod_visibility" name="visibilitytype">
                                            <optgroup label="Visibility">
                                                {{#select products.visibilitytype}}
                                                <option value="true" selected="selected">Show</option>
                                                <option value="false">Hide</option>
                                                {{/select}}
                                            </optgroup>
                                        </select>
                                        <span class="form_error">Please enter the Product visibility</span>
                                    </label>
                                </span>
                            </span>
                        </h2>
                        <h2 class="hndle ui-sortable-handle">
                            <span>Type of product<span class="type_box ">
                                    <label for="featured-type">
                                        <select id="featured-type" name="featured">
                                            <optgroup label="Type">
                                                {{#select products.featured}}
                                                <option value="false">Normal</option>
                                                <option value="true">Featured</option>
                                                {{/select}}
                                            </optgroup>
                                        </select>
                                    </label>
                                </span>
                            </span>
                        </h2>
                        <div class="inside">
                            <div class="panel-wrap product_data">
                                <ul class="product_data_tabs wc-tabs">
                                    <li class="general_options general_tab">
                                        <a href="javascript:void(0)" data-tab-id="general_product_data"
                                            class="active btn-secondary"><span>General</span></a>
                                    </li>
                                    <li class="inventory_options inventory_tab">
                                        <a href="javascript:void(0)" data-tab-id="inventory_product_data"
                                            class="btn-secondary"><span>Inventory</span></a>
                                    </li>
                                    <li class="shipping_options shipping_tab">
                                        <a href="javascript:void(0)" data-tab-id="shipping_product_data"
                                            class="btn-secondary"><span>Shipping</span></a>
                                    </li>
                                    <li class="attribute_options attribute_tab"
                                        {{#equal products.producttype "simple" }} style="display: none" {{/equal}}>
                                        <a href="javascript:void(0)" data-tab-id="product_attributes"
                                            class="btn-secondary"><span>Attributes</span></a>
                                    </li>
                                    <li class="variations_options variations_tab"
                                        {{#equal products.producttype "simple" }} style="display: none" {{/equal}}>
                                        <a href="javascript:void(0)" data-tab-id="variable_product_options"
                                            class="btn-secondary"><span>Variations</span></a>
                                    </li>
                                </ul>
                                <div id="general_product_data" class="panel woocommerce_options_panel"
                                    style="display: block;">
                                    <div class="options_group_pricing">
                                        <p class="form-field _regular_price_field ">
                                            <label for="e_prod_dregular_price">Default Regular price ($) <span
                                                    class="required">*</span></label>
                                            <input type="number" class="short wc_input_price" style=""
                                                name="regular_price" id="e_prod_dregular_price"
                                                value="{{products.dregularprice}}" placeholder="" step=".01">
                                        </p>
                                        <p class="form-field _sale_price_field ">
                                            <label for="e_d_sale_price">Default Sale price ($) <span
                                                    class="required">*</span></label>
                                            <input type="number" class="short wc_input_price" style="" name="sale_price"
                                                id="e_d_sale_price" value="{{products.dsaleprice}}" placeholder=""
                                                step=".01">
                                            <span class="form_error">Sale Price should be Less then Regular Price</span>
                                        </p>
                                        <p class="form-field _sale_price_field clearfix">
                                            <label for="">Total CBD (mg) <span class="required"></span></label>
                                            <input type="number" class="short wc_input_price" style="" name="totalcbdmg"
                                                id="" value="{{products.totalcbdmg}}" placeholder="Total CBD"
                                                step=".01">

                                        </p>
                                        <p class="form-field clearfix">
                                            <label for="">CBD Per Unit mg<span class="required"></span></label>
                                            <input type="number" class="short" style="" name="cbdperunitmg" id=""
                                                value="{{products.cbdperunitmg}}" placeholder="CBD Per Unit Mg"
                                                step=".01">
                                        </p>
                                        <p class="form-field clearfix">
                                            <label for="">Servings<span class="required"></span></label>
                                            <input type="number" class="short" style="" name="servings" id=""
                                                value="{{products.servings}}" placeholder="Servings" step=".01">
                                        </p>
                                        <p class="form-field clearfix">
                                            <label for="">Serving Size<span class="required"></span></label>
                                            <input type="text" class="short" style="" name="servingsize" id=""
                                                value="{{products.servingsize}}" placeholder="Serving Size">
                                        </p>
                                           <p class="form-field clearfix">
                                            <label for="">Field Name<span class="required"></span></label>
                                            <input type="text" class="short" style="" name="fieldname" id=""
                                                value="{{products.fieldname}}" placeholder="Field Name" >
                                        </p>
                                        <p class="form-field clearfix">
                                            <label for="">Field Value<span class="required"></span></label>
                                            <input type="text" class="short" style="" name="fieldvalue" id=""
                                                value="{{products.fieldvalue}}" placeholder="Field Value" >
                                        </p>
                                        <p class="form-field clearfix">
                                            <label for="">Barcode: <span class="required"></span></label>
                                            <input type="number" class="short" style="" name="barcode" id=""
                                                value="{{products.barcode}}" placeholder="Barcode">
                                        </p>
                                    </div>
                                    <div class="options_group reviews">
                                        <p class="form-field enable_review_field ">
                                            <label for="enable_review">Enable reviews</label>
                                            <input type="checkbox" class="checkbox" style="" data-toggle="toggle"
                                                name="enable_review" id="enable_review" value="true"
                                                {{#if products.enablereview}}checked="checked" {{/if}}>
                                        </p>
                                    </div>
                                </div>
                                <div id="inventory_product_data" class="panel woocommerce_options_panel"
                                    style="display: none;">
                                    <div class="options_group">
                                        <p class="form-field _sku_field ">
                                            <label for="_sku">
                                                <abbr title="Stock Keeping Unit">SKU</abbr> <span
                                                    class="required">*</span>
                                            </label>
                                            <input type="text" class="short" name="sku" id="edit_prod_sku"
                                                value="{{products.productid.sku}}" placeholder="SKU">
                                            <span class="form_error">Please enter the Product SKU</span>
                                        </p>
                                        <p class="form-field _manage_stock_field">
                                            <label for="_manage_stock">Manage stock?</label>
                                            <input type="checkbox" class="checkbox manage_stock" name="manage_stock"
                                                value="1" {{#if products.managestockstatus}}checked="checked" {{/if}}>
                                        </p>
                                        {{#if products.managestockstatus}}

                                        {{/if}}
                                        <div class="stock_fields" style="display: block;">
                                            <p class="form-field _stock_field ">
                                                <label for="_stock">Stock quantity</label>
                                                <input type="number" class="short wc_input_stock" style="" name="stock"
                                                    id="_stock" value="{{products.unit}}" placeholder="" step="any">
                                            </p>
                                            {{!-- <input type="hidden" name="original_stock" value="0">
                                            <p class="form-field _low_stock_amount_field ">
                                                <label for="_low_stock_amount">Low stock threshold</label>
                                                <input type="number" class="short" style="" name="low_stock_amount" id="_low_stock_amount" value="" placeholder="2" step="any"> 
                                            </p> --}}
                                        </div>
                                        <p class="stock_status_field form-field _stock_status_field"
                                            style="display: block;">
                                            <label for="_stock_status">Stock status</label>
                                            <select id="_stock_status" name="_stock_status" class="select short">
                                                <option value="instock"
                                                    {{#equal products.stock_status "instock"}}selected="selected"
                                                    {{/equal}}>In stock</option>
                                                <option value="outofstock"
                                                    {{#equal products.stock_status "outofstock"}}selected="selected"
                                                    {{/equal}}>Out of stock</option>
                                            </select>
                                        </p>
                                    </div>

                                    {{!-- <div class="options_group" style="display: block;">
                                        <p class="form-field _sold_individually_field" style="display: block;">
                                            <label for="_sold_individually">Sold individually</label><input type="checkbox" class="checkbox" style=""
                                                name="_sold_individually" id="_sold_individually" value="yes"> <span class="description">Enable this to
                                                only allow one of this item to be bought in a single order</span></p>
                                    </div> --}}
                                </div>
                                <div id="shipping_product_data" class="panel woocommerce_options_panel"
                                    style="display: none;">
                                    <div class="options_group">
                                        <p class="form-field _weight_field clearfix">
                                            <label for="_weight">Batch No.</label>
                                            <input type="text" class="product_shipping_batch" name="batch_no"
                                                value="{{products.batch_no}}" placeholder="0">
                                        </p>
                                        <p class="form-field _weight_field clearfix">
                                            <label for="_weight">Expiry</label>
                                            <input type="text" class="product_expiry" name="expiry"
                                                value="{{products.expiry}}" placeholder="dd/mm/yyyy">
                                        </p>
                                        <p class="form-field dimensions_field" >
                                            <label for="product_length" style="width:35%;">Dimensions (inches)</label>
                                            <span class="wrap" style="float: left;">
Length:
                                                <input style="width: 90px;"id="product_length" placeholder="Length"
                                                    class="input-text wc_input_decimal" type="number" name="_length"
                                                    value="{{products.shipping_length}}">
Width:
                                                <input style="width: 90px;" placeholder="Width" class="input-text wc_input_decimal"
                                                    type="number" name="_width" value="{{products.shipping_width}}">
Height:                                                
<input placeholder="Height" style="width: 90px;" class="input-text wc_input_decimal last"
                                                    type="number" name="_height" value="{{products.shipping_height}}">
                                            </span>
                                        </p>
                                        <p class="form-field dimensions_field clearfix">
                                            <label for="product_length">Volume</label>
                                            <span class="wrap">
                                                <input placeholder="Volume" class="product_volume" type="number"
                                                    name="volume" value="{{products.volume}}">
                                                <select name="volume_unit" class="product_vol_unit">
                                                    <option value="cc"
                                                        {{#equal products.volumeunit "cc"}}selected="selected"
                                                        {{/equal}}>CC</option>
                                                    <option value="ml"
                                                        {{#equal products.volumeunit "ml"}}selected="selected"
                                                        {{/equal}}>ML</option>
                                                    <option value="softgel"
                                                        {{#equal products.volumeunit "softgel"}}selected="selected"
                                                        {{/equal}}>soft gel</option>
                                                    <option value="capsule"
                                                        {{#equal products.volumeunit "capsule"}}selected="selected"
                                                        {{/equal}}>Capsule</option>
                                                    <option value="oz"
                                                        {{#equal products.volumeunit "oz"}}selected="selected"
                                                        {{/equal}}>oz</option>
                                                    <option value="lb"
                                                        {{#equal products.volumeunit "lb"}}selected="selected"
                                                        {{/equal}}>lb</option>
                                                </select>
                                            </span>
                                        </p>
                                               <p class="form-field clearfix">
                                            <label for="">Weight (oz)</label>
                                            <input type="number" class="" name="weight" value="{{products.weight}}"
                                                placeholder="0">
                                        </p>
                                    </div>
                                    {{!-- <div class="options_group">
                                        <p class="form-field shipping_class_field">
                                            <label for="product_shipping_class">Shipping class</label>
                                            <select name="product_shipping_class" id="product_shipping_class" class="select short">
                                                <option value="-1" selected="selected">No shipping class</option>
                                            </select>
                                            <span class="woocommerce-help-tip"></span>
                                        </p>
                                    </div> --}}
                                </div>
