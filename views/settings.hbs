<style>
  [data-acc-toggle] {
    cursor: pointer;
    background-color: #f1f1f1;
    position: relative;
  }

  [data-acc-toggle]:after {
    content: "";
    display: block;
    position: absolute;
    right: 20px;
    top: 0;
    bottom: 0;
    margin: auto;
    width: 10px;
    height: 10px;
    border: 1px solid transparent;
    border-bottom-color: #000;
    border-right-color: #000;
    transform: translateY(-25%) rotate(45deg);
    transition: all .3s;
  }

  [data-acc-id] {
    display: none;
  }

  .opened [data-acc-toggle]:after {
    transform: translateY(25%) rotate(225deg);
  }

  .accordian-wrapper {
    border: 1px solid #f1f1f1;
    margin-bottom: 20px;
    display: block;
    overflow: hidden;
    border-radius: 4px;
    box-shadow: 0px 0px 0px rgba(0, 0, 0, .08);
    transition: all .3s;
  }

  .accordian-wrapper.opened {
    box-shadow: 0px 5px 15px rgba(0, 0, 0, .08);
  }

  .accordian-wrapper>div {
    padding: 10px;
  }

  .has-preview .preview-block {
    display: none;
  }

  .has-preview .preview-checked:checked~.preview-block {
    display: block;
  }

  /*---*/
  .fixed-btn {
    position: fixed;
    opacity: .8;
    bottom: 40px;
    right: 40px;
    z-index: 9999;
    box-shadow: 0px 10px 25px #00000036;
  }

  .fixed-btn:hover {
    opacity: 1
  }
</style>
